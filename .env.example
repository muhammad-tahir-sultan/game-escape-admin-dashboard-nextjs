# MongoDB Atlas Connection
MONGODB_URI=your_mongodb_atlas_connection_string

# Application Secret for Sessions (Required for Passport/Custom sessions)
SESSION_SECRET=your_random_secure_secret_at_least_32_chars

# Application URL (If you use it for absolute redirects, though relative is used in middleware)
APP_URL=https://your-domain.vercel.app

# Optional: Cloudinary Configuration (if using for uploads)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Note: This project uses Passport.js, not NextAuth, 
# so NEXTAUTH_URL and NEXTAUTH_SECRET from traditional boilerplates are NOT required.
